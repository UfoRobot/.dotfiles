defaults

account work-account
host @@MAIL_HOST@@
port 587
protocol smtp
from @@MAIL_ADDRESS@@
auth on
user @@MAIL_USER@@
passwordeval /usr/bin/security find-internet-password -w -a @@MAIL_USER@@ -s @@MAIL_HOST@@ ~/Library/Keychains/login.keychain
tls on
tls_certcheck off

account default: work-account
